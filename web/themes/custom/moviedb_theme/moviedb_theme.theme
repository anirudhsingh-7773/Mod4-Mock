/**
 * @file
 * Movie DB Theme theme file.
 */

function moviedb_theme_theme_suggestions_views_view_unformatted_alter(array &$suggestions, array $variables) {
  if ($variables['view']->id() == 'movie_banner') {
    $suggestions[] = 'views_view_unformatted__' . $variables['view']->id();
  }
}
